[tdkbtr181Parameters_WIFI.config]

#Number of configured namespaces for WiFi module
WIFI_PARAMETER_COUNT=19

#Configure namespaces, datatype of value,default value and expected range of values, eg : WIFI_PARAMETER_1=Device.WiFi.SSID.1.Enable|bool|true|true;false
