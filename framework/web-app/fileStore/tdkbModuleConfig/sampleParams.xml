               <!-- whether set value will be retained and set should be followed by a get or not -->
               <persistentSet>true</persistentSet>
               <!--default value of the param after factory reset -->
               <defaultValue>false</defaultValue>
               <!--default value exepcted by snmp after factory reset -->
               <snmpDefaultValue>2</snmpDefaultValue>
               <!-- the range of values acceptable -->
               <expectedValues>true,false</expectedValues>
	       <!-- the range of values expected by snmp -->
               <snmpExpectedValues>1,2</snmpExpectedValues>
        </parameter>
</parameters>
