<?xml version="1.0" encoding="UTF-8"?>
<parameters>
        <parameter>
                <!--the tr-181 parameter name -->
               <name></name>
               <!--OID of the above parameter if available -->
               <oid></oid>
               <!--webpa param name of the above tr-181 parameter if available -->
               <webpaName></webpaName>
               <!--type of the parameter -->
               <type></type>
               <!--type of the parameter expected by snmp. This tag is required only if snmp is expecting a different datatype name-->
               <snmpType></snmpType>
               <!--whether set is applicable or not -->
               <writable></writable>
               <!--whether snmp set is applicable or not. This tag is required only if snmp set validation should be skipped for an OID -->
               <snmpWritable>false</snmpWritable>
               <!-- whether set value will be retained and set should be followed by a get or not -->
               <persistentSet></persistentSet>
               <!--default value of the param after factory reset -->
               <defaultValue></defaultValue>
               <!--default value exepcted by snmp after factory reset . This tag is required only if the default value expected by snmp is different-->
               <snmpDefaultValue></snmpDefaultValue>
               <!-- default value of the param after factory reset for WEBPA. This tag is required only if the default value expected by webpa is different-->
               <webpaDefaultValue></webpaDefaultValue>
               <!-- the range of values acceptable -->
               <expectedValues></expectedValues>
               <!-- the range of values expected by snmp. This tag is required only if the values expected by snmp is different -->
               <snmpExpectedValues></snmpExpectedValues>
               <!-- the range of values acceptable for WEBPA. This tag is required only if the values expected by webpa is different-->
               <webpaExpectedValues></webpaExpectedValues>
	       <!--to be specified only if expectedValues is not available. -->
	       <setValue></setValue>
               <!--to be specified as true if the parameter's value WILL NOT persist on reboot. This tag is required only if the parameter is not persistent on reboot-->
               <RFCEnabled></RFCEnabled>
        </parameter>

	<!-- a sample parameter data from PAM module is shown below -->
