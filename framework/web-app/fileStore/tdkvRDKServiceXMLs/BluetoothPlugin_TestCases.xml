<testPlugin pluginName="Bluetooth" pluginVersion="1">

        <pluginPreRequisite>
          <prerequisite requisiteId="1" requisiteName="Activate_Plugins">
            <testStep testStepId="1" name="Check_PluginActive_Status" method="Get_Plugin_Status" saveResult="true" pluginName="Controller">
                <params>
                  <param tag="plugin" value="org.rdk.Bluetooth" indexOnly="true" />
                </params>
                <resultGeneration useMethodTag="controller_get_plugin_state" arguments="check_status,org.rdk.Bluetooth" />
            </testStep>
            <testStep testStepId="2" name="Activate_Plugin" method="Activate_Plugin"  pluginName="Controller" >
                <conditionalExecution enable="true" Id="1" useMethodTag="controller_get_plugin_state" arguments="isDeactivated,org.rdk.Bluetooth" />
                <params>
                  <param tag="callsign" value="org.rdk.Bluetooth" />
                </params>
            </testStep>
            <testStep testStepId="3" name="Check_PluginActive_Status" method="Get_Plugin_Status" pluginName="Controller"  >
                <conditionalExecution enable="true" Id="1" useMethodTag="controller_get_plugin_state" arguments="isDeactivated,org.rdk.Bluetooth" />
                <params>
                  <param tag="plugin" value="org.rdk.Bluetooth" indexOnly="true" />
                </params>
                <resultGeneration useMethodTag="controller_get_plugin_state" arguments="check_status,org.rdk.Bluetooth" expectedValues="activated"/>
            </testStep>
          </prerequisite>
          <prerequisite requisiteId="2" requisiteName="Activate_System_Plugin">
            <testStep testStepId="1" name="Check_PluginActive_Status" method="Get_Plugin_Status" saveResult="true" pluginName="Controller">
                <params>
                  <param tag="plugin" value="org.rdk.System" indexOnly="true" />
                </params>
                <resultGeneration useMethodTag="controller_get_plugin_state" arguments="check_status,org.rdk.System" />
            </testStep>
            <testStep testStepId="2" name="Activate_Plugin" method="Activate_Plugin"  pluginName="Controller" >
                <conditionalExecution enable="true" Id="1" useMethodTag="controller_get_plugin_state" arguments="isDeactivated,org.rdk.System" />
                <params>
                  <param tag="callsign" value="org.rdk.System" />
                </params>
            </testStep>
            <testStep testStepId="3" name="Check_PluginActive_Status" method="Get_Plugin_Status" pluginName="Controller"  >
                <conditionalExecution enable="true" Id="1" useMethodTag="controller_get_plugin_state" arguments="isDeactivated,org.rdk.System" />
                <params>
                  <param tag="plugin" value="org.rdk.System" indexOnly="true" />
                </params>
                <resultGeneration useMethodTag="controller_get_plugin_state" arguments="check_status,org.rdk.System" expectedValues="activated"/>
            </testStep>
          </prerequisite>      
  
          <prerequisite requisiteId="3" requisiteName="Bluetooth_Stack_Enable">
            <testStep testStepId="1" name="Enable_Bluetooth_Stack" method="Enable_Stack" saveResult="true">
                  <resultGeneration useMethodTag="bluetooth_set_operation" />
            </testStep>
          </prerequisite>

          <prerequisite requisiteId="4" requisiteName="Register_And_Listen_Events" type="eventRegister" trace="false">
            <event pluginName="Bluetooth" eventName="Event_On_Discovered_Device" />  
            <event pluginName="Bluetooth" eventName="Event_On_Status_Changed" />
            <event pluginName="Bluetooth" eventName="Event_On_Request_Failed" />
          </prerequisite>

        </pluginPreRequisite>

        <testCase testCaseId="BT_01" testCaseName= "Set_And_Get_Name"  desc="Check whether name of the device can be set and get">
            <testCaseInfo testCaseType="setandget" revert="true"/>
            <testStep testStepId="1" name="Get_Bluetooth_Device_Name" method="Get_Name" saveResult="true" revertGet="yes" >
              <resultGeneration useMethodTag="bluetooth_get_name" />
            </testStep>
            <testStep testStepId="2" name="Set_Bluetooth_Device_Name" method="Set_Name" revertSet="yes">
                <params>
                  <param tag="name"   value="Test Value" revertParam="yes" />
                </params>
                <resultGeneration useMethodTag="bluetooth_set_operation" />
            </testStep>
            <testStep testStepId="3" name="Get_Bluetooth_Device_Name" method="Get_Name" revertFlag="yes">
                <resultGeneration useMethodTag="bluetooth_get_name" expectedValues="Test Value" arguments="check_name"/>
            </testStep>
        </testCase>

        <testCase testCaseId="BT_02" testCaseName="Bluetooth_Toggle_Discoverable_Status" desc="Toggle bluetooth discoverable status" >
           <testCaseInfo testCaseType="setandget" revert="true" />
            <testStep testStepId="1" name="Is_Bluetooth_Device_Discoverable" method="Is_Discoverable" saveResult="true" revertGet="yes">
                <resultGeneration useMethodTag="bluetooth_get_discoverable_status"/>
            </testStep>
            <testStep testStepId="2" name="Set_Bluetooth_Device_Discoverable" method="Set_Discoverable" revertSet="yes">
                <params>
                  <param tag="discoverable" usePreviousResult="true" Id="1" useMethodTag="bluetooth_toggle_discoverable_status" revertParam="yes" />
                  <param tag="timeout"   useDefault="true"/>
                </params>
                <resultGeneration useMethodTag="bluetooth_set_operation" />
            </testStep>
            <testStep testStepId="3" name="Is_Bluetooth_Device_Discoverable" method="Is_Discoverable" revertFlag="yes">
                <resultGeneration useMethodTag="bluetooth_get_discoverable_status">
                    <expectedValues usePreviousResult="true" Id="1" useMethodTag="bluetooth_toggle_discoverable_status" />
                </resultGeneration>
            </testStep>
        </testCase>

        <testCase testCaseId="BT_03" testCaseName="Bluetooth_Get_Discovered_Devices" desc="Scan and get discovered devices using onDiscoveredDevice Event">
            <testCaseInfo testCaseType="direct" configurableTest="true" testKey="BT_EMULATOR_SUPPORT" useMethodTag="bt_na_tests" arguments="no"/>
            <testStep testStepId="1" name="Execute_BT_Ctrl" action="externalFnCall" >
                <resultGeneration useMethodTag="executeBluetoothCtl" arguments="bluetoothctl,agent NoInputNoOutput,default-agent,discoverable on"/>
            </testStep>
            <testStep testStepId="2" name="Start_Scan" method="Start_Scan" >
                <params>
                  <param tag="timeout" useDefault="true" />
                  <param tag="profile" value="LOUDSPEAKER" />
                </params>
                <resultGeneration useMethodTag="bluetooth_set_operation" />
            </testStep>
            <testStep testStepId="3" name="Check_On_Status_Changed_Event" event="Event_On_Status_Changed" action="eventListener" delay="3">
                <resultGeneration useMethodTag="bluetooth_check_status_changed_event"  expectedValues="DISCOVERY_STARTED"/>
            </testStep>
            <testStep testStepId="4" name="Check_On_Discovered_Device_Event" event="Event_On_Discovered_Device" action="eventListener" delay="20">
                <resultGeneration useMethodTag="bluetooth_check_discovered_device_event" arguments="get_devices_info" />
            </testStep>
            <testStep testStepId="5" name="Check_On_Status_Changed_Event" event="Event_On_Status_Changed" action="eventListener" delay="10">
                <resultGeneration useMethodTag="bluetooth_check_status_changed_event"  expectedValues="DISCOVERY_COMPLETED"/>
            </testStep>
            <testStep testStepId="6" name="Get_Discovered_Devices" method="Get_Discovered_Devices" >
                <resultGeneration useMethodTag="bluetooth_get_discovered_devices" arguments="get_devices_info" />
            </testStep>
            <testStep testStepId="7" name="Stop_Scan" method="Stop_Scan" >
                <resultGeneration useMethodTag="bluetooth_set_operation" />
            </testStep>
        </testCase>

        <testCase testCaseId="BT_04" testCaseName="Bluetooth_Pair_Device" desc="Pairs to the given bluetooth device">
  	        <testCaseInfo testCaseType="direct" configurableTest="true" testKey="BT_EMULATOR_SUPPORT" useMethodTag="bt_na_tests" arguments="no"/>
            <testStep testStepId="1" name="Execute_BT_Ctrl" action="externalFnCall" >
                <resultGeneration useMethodTag="executeBluetoothCtl" arguments="bluetoothctl,agent NoInputNoOutput,default-agent,discoverable on"/>
            </testStep>
            <testStep testStepId="2" name="Start_Scan" method="Start_Scan" >
                <params>
                  <param tag="timeout" useDefault="true" />
                  <param tag="profile" value="LOUDSPEAKER"/>
                </params>
                <resultGeneration useMethodTag="bluetooth_set_operation" />
            </testStep>
            <testStep testStepId="3" name="Stop_Scan" method="Stop_Scan" delay="30">
                <resultGeneration useMethodTag="bluetooth_set_operation" />
            </testStep>
            <testStep testStepId="4" name="Get_Discovered_Devices" method="Get_Discovered_Devices" saveResult="true">
	            <resultGeneration useMethodTag="bluetooth_get_discovered_devices" arguments="get_device_id" >
		            <expectedValues useConfigFile="true" key="BT_EMU_DEVICE_NAME"/>
		        </resultGeneration>
            </testStep>
	        <testStep testStepId="5" name="Pair" method="Pair" >
                <params>
                  <param tag="deviceID" usePreviousResult="true" Id="4" useMethodTag="bluetooth_get_device_id" revertParam="yes" />
                </params>
                <resultGeneration useMethodTag="bluetooth_set_operation" expectedValues="true"/>
            </testStep>
            <testStep testStepId="6" name="Get_Paired_Devices" method="Get_Paired_Devices" delay="5">
                <resultGeneration useMethodTag="bluetooth_get_paired_devices" arguments="check_device_paired" >
                    <expectedValues useConfigFile="true" key="BT_EMU_DEVICE_NAME" />
                </resultGeneration>
            </testStep>
            <testStep testStepId="7" name="Unpair" method="Unpair" delay="3">
                <params>
                  <param tag="deviceID" usePreviousResult="true" Id="4" useMethodTag="bluetooth_get_device_id" revertParam="yes" />
                </params>
                <resultGeneration useMethodTag="bluetooth_set_operation" expectedValues="true"/>
	        </testStep> 
        </testCase>

